#!/usr/bin/env bash
source "$DOTFILES/sources/strict.bash"

function help {
	cat <<-EOF >/dev/stderr
		USAGE:
		echo-filenames ./*
		# ^ for each argument or stdin, output the filename without extension
	EOF
}

function act {
	fs-filename --basename "$1"
}

source "$DOTFILES/sources/stdinargs.bash"
