#!/usr/bin/env bash
source "$DOTFILES/sources/strict.bash"

function act {
	if is-number "$1"; then
		echo "$1"
	else
		echo-style --error="[$1] is not a number" >/dev/stderr
		return 22 # Invalid argument
	fi
}

function noact {
	return 96 # No message available on STREAM
}

source "$DOTFILES/sources/stdinargs.bash"
